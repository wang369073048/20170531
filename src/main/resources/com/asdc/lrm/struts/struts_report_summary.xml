<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="report_summary" extends="interceptor" namespace="/report_summary">
    	<action name="totality" class="com.asdc.lrm.action.ReportSummaryAction" method="totality">
        	<result name="success">/modules/statistics/summary/report_totality.jsp</result>
        </action>
    	
        <action name="declareConstruction" class="com.asdc.lrm.action.ReportSummaryAction" method="declareConstruction">
        	<result name="success">/modules/statistics/summary/report_declareConstruction.jsp</result>
        </action>
        
        <action name="exprot_declareConstruction" class="com.asdc.lrm.action.ReportSummaryAction" method="exprot_declareConstruction">
        	<result name="success">/modules/statistics/summary/export_declareConstruction.jsp</result>
        </action>
        
        <action name="distribution" class="com.asdc.lrm.action.ReportSummaryAction" method="distribution">
        	<result name="success">/modules/statistics/summary/report_distribution.jsp</result>
        </action>
    </package>
</struts>