<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
        
<hibernate-mapping>

	<class name="com.asdc.lrm.entity.ZykCityEntity" table="zyk_city">
		<id name="cityId">
			<column name="cityId" sql-type="bigint(11)"/>
			<generator class="native" />
		</id>
		<property name="name" column="name" type="string" length="100" not-null="true"/>
		<property name="type" column="type" type="integer" not-null="true"/>
		<property name="parentId" column="parentId" type="string" length="100"/>
	</class>

</hibernate-mapping>