<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
        
<hibernate-mapping>

	<class name="com.asdc.lrm.entity.ZykCourseRresourceRelationEntity" table="zyk_course_resource_relation">
		<id name="relationId">
			<column name="relationId" sql-type="varchar(100)"/>
			<generator class="assigned" />
		</id>
		<property name="zykId" column="zykId" type="string" length="100" not-null="true"/>
		<property name="courseId" column="Courseid" type="string" length="100" />
		<property name="courseModuleId" column="CourseModuleId" type="string" length="100"/>
		<property name="courseModule" column="courseModule" type="string" length="100" />
		<property name="resourceId" column="Resourceid" type="string" length="100" />
	</class>

</hibernate-mapping>