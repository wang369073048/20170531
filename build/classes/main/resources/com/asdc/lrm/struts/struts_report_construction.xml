<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="report_construction" extends="interceptor" namespace="/report_construction">
    	
    	<!-- 用户数量统计 -->
        <action name="reportUserCount" class="com.asdc.lrm.action.ReportConstructionAction" method="reportUserCount">
        	<result name="success">/modules/statistics/construction/report/reportUserCount.jsp</result>
        </action>
        
        <!-- 用户院校分布 -->
        <action name="reportInstituteCount" class="com.asdc.lrm.action.ReportConstructionAction" method="reportInstituteCount">
        	<result name="success">/modules/statistics/construction/report/reportInstituteCount.jsp</result>
        </action>
        
        <!-- 用户地域分布 -->
        <action name="reportProvinceCount" class="com.asdc.lrm.action.ReportConstructionAction" method="reportProvinceCount">
        	<result name="success">/modules/statistics/construction/report/reportProvinceCount.jsp</result>
        </action>
        
        <!-- 资源素材总数 -->
        <action name="reportResourceCount" class="com.asdc.lrm.action.ReportConstructionAction" method="reportResourceCount">
        	<result name="success">/modules/statistics/construction/report/reportResourceCount.jsp</result>
        </action>
        
        <!-- 教学应用资源分布 -->
        <action name="reportInstructionCount" class="com.asdc.lrm.action.ReportConstructionAction" method="reportInstructionCount">
        	<result name="success">/modules/statistics/construction/report/reportInstructionCount.jsp</result>
        </action>
        
        <!-- 本专业知识点 -->
        <action name="reportKnowledgeCount" class="com.asdc.lrm.action.ReportConstructionAction" method="reportKnowledgeCount">
        	<result name="success">/modules/statistics/construction/report/reportKnowledgeCount.jsp</result>
        </action>
        
        <!-- 课程数 -->
        <action name="reportCourseCount" class="com.asdc.lrm.action.ReportConstructionAction" method="reportCourseCount">
        	<result name="success">/modules/statistics/construction/report/reportCourseCount.jsp</result>
        </action>
        
        <!-- 不同层级课程分布  -->
        <action name="reportCourseLevelCount" class="com.asdc.lrm.action.ReportConstructionAction" method="reportCourseLevelCount">
        	<result name="success">/modules/statistics/construction/report/reportCourseLevelCount.jsp</result>
        </action>
        
        <!-- 不同类型课程分布  -->
        <action name="reportCourseTypeCount" class="com.asdc.lrm.action.ReportConstructionAction" method="reportCourseTypeCount">
        	<result name="success">/modules/statistics/construction/report/reportCourseTypeCount.jsp</result>
        </action>
        
        <!-- 各门课程模块分布 -->
        <action name="reportCourseModuleCount" class="com.asdc.lrm.action.ReportConstructionAction" method="reportCourseModuleCount">
        	<result name="success">/modules/statistics/construction/report/reportCourseModuleCount.jsp</result>
        </action>
        
        <!-- 课程引用的资源素材占所有资源素材比 -->
        <action name="reportCourseQuoteResource" class="com.asdc.lrm.action.ReportConstructionAction" method="reportCourseQuoteResource">
        	<result name="success">/modules/statistics/construction/report/reportCourseQuoteResource.jsp</result>
        </action>
        
        <!-- 题库题目总数 -->
        <action name="reportQuestionCount" class="com.asdc.lrm.action.ReportConstructionAction" method="reportQuestionCount">
        	<result name="success">/modules/statistics/construction/report/reportQuestionCount.jsp</result>
        </action>
    </package>
</struts>