<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
	<class name="com.asdc.lrm.entity.SysMenuRoleEntity" table="lrm_menurole">
		<id name="id">
			<column name="ID" sql-type="bigint(11)"/>
			<generator class="native" />
		</id>
		<property name="menuId"><column name="MENU_ID" sql-type="bigint(11)" not-null="true"/></property>
		<property name="roleId"><column name="ROLE_ID" sql-type="bigint(11)" not-null="true"/></property>
	</class>

</hibernate-mapping>