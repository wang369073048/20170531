<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
        
<hibernate-mapping>

	<class name="com.asdc.lrm.entity.ZykQuestionbankEntity" table="zyk_questionbank">
		<id name="qbId">
			<column name="qbId" sql-type="varchar(100)"/>
			<generator class="assigned" />
		</id>
		<property name="zykId" column="zykId" type="string" length="100" not-null="true"/>
		<property name="title" column="Title" type="string" length="100" />
		<property name="questionNum" column="QuestionNum" type="integer" length="11" />
		<property name="objQuesNum" column="ObjQuesNum" type="integer" length="11" />
		<property name="subQuesNum" column="SubQuesNum" type="integer" length="11" />
		<property name="citedQuesNum" column="CitedQuesNum" type="integer" length="11" />
		<property name="quesUsingNum" column="QuesUsingNum" type="integer" length="11" />
	</class>

</hibernate-mapping>